    <script>
        function debounce(fn, delay) {
            let timer = null;
            return function () {
                let context = this, args = arguments;
                clearTimeout(timer);
                timer = setTimeout(function () {
                    fn.apply(context, args);
                }, delay);
            };
        }

        $('#search_input').keyup(debounce(function (e) {
            if (!e.target.value) return;
            $.getJSON("/api/query-search-extended/?q=" + e.target.value, function (data) {
                let table = '';
                $.each(data, function (i, item) {
                    let icon = item['image'] ? ' style="background-image: url(' + item['image'] + ')" ' : '';
                    table += '<tr>\
                        <td class="icon"><div ' + icon + ' ></div></td>\
                        <td>\
                            <a href="' + item['link'] + '">\
                            ' + item['code'] + '.' + item['exchange'] + '\
                            </a>\
                        </td>\
                        <td>\
                            <a href="' + item['link'] + '">\
                            ' + item['name'] + '\
                            </a>\
                        </td>\
                        <td class="val">' + item['open'] + '</td>\
                        <td class="val">' + item['high'] + '</td>\
                        <td class="val">' + item['low'] + '</td>\
                        <td class="val">' + item['close'] + '</td>\
                        <td class="val">&nbsp;</td>\
                        <td class="links">\
                            <a class="download" href="/api/sample/' + item['code'] + '.' + item['exchange'] + '" download></a>\
                            <a class="arrow" href="' + item['link'] + '"></a>\
                        </td>\
                    </tr>';
                    if (i > 6) return false;
                });
                $('.search_box .data_table tbody').html(table);
            });
        }, 400));
    </script>

